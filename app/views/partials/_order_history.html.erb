<div class="card">
  <div class="card-content">      
      
      <h5> Order History: </h5>
      <table class="table table-striped">

        <thead>
          <tr>
            <th> Date</th>
            <th> Amount</th>
          </tr>
        </thead>
          <% @previous_orders.each do |o| %>
            <tr>
              <td><%= link_to "#{o.date.strftime('%m/%d/%y')}", order_path(o) %></td>
              <td> <%= number_to_currency(o.grand_total).to_s %></td>
            </tr>
          <% end %>
      </table>

  </div>
</div>